<div class="header">
	<%= image_tag("techtool_header.png", :alt => "Header") %>
</div>
<div class="sign_in">
	<%= form_for("user", :url => "#") do |f| %>
		<p><%= f.label :email %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		<%= f.text_field :email %></p>
		<p><%= f.label :password %> 
		<%= f.password_field :password %></p>
		<%= f.check_box :remember_me %> <%= f.label :remember_me %></p>&nbsp;&nbsp;&nbsp;
		<%= f.submit 'Sign in' %>
	<% end %>	
</div>